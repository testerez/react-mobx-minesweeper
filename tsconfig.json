{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": false,
    "jsx": "react",
    "sourceMap": true,
    "strictNullChecks": true,
    "pretty": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "types": [
      "browser"
    ]
  },
  "awesomeTypescriptLoaderOptions": {
  },
  "exclude": [
    "node_modules"
  ]
}
