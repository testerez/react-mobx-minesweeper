    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.32",
    "@types/react-dom": "^16.0.4",
    "classnames": "^2.2.5",
    "core-decorators": "^0.20.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.5",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
