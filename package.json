{
  "name": "react-ts",
  "version": "1.0.0",
  "description": "A react starter project using typescript",
  "main": "dist/index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html  --public-url '.'",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "karma start  --single-run --grep",
    "test:w": "karma start --grep",
    "check": "npm run check:ts && npm run lint",
    "check:ts": "tsc --noEmit",
    "lint": "tslint -r node_modules/tslint-microsoft-contrib 'src/**/*.ts' 'src/**/*.tsx'",
    "tsfmt": "find src | grep '\\.tsx?$' -E | xargs tsfmt -r"
  },
  "author": "Tom Esterez",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/enzyme": "^3.1.9",
    "@types/mocha": "^2.2.32",
    "@types/react": "^16.0.40",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^3.5.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.0.4",
    "install": "^0.10.4",
    "json-loader": "^0.5.7",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^2.0.13",
    "mobx-react-devtools": "^4.2.15",
    "mocha": "^5.0.1",
    "node-sass": "^4.7.2",
    "npm": "^5.7.1",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss-loader": "^2.1.1",
    "precss": "^3.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.0.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.4.2",
    "style-loader": "^0.20.2",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.0.3",
    "typescript-formatter": "^7.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-build-notifier": "^0.1.23",
    "webpack-dev-server": "^3.1.0"
  },
