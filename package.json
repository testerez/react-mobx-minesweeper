{
  "name": "react-ts",
  "version": "1.0.0",
  "description": "A react starter project using typescript",
  "main": "dist/index.html",
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "webpack",
    "deploy": "npm run build && surge -p public -d somedomain.com",
    "test": "karma start  --single-run --grep",
