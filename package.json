    "@types/core-js": "^0.9.46",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/react-dom": "^16.0.4",
    "classnames": "^2.2.5",
    "core-decorators": "^0.20.0",
    "core-js": "^2.5.3",
    "lodash": "^4.17.5",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.2",
    "parcel-bundler": "^1.6.2",
