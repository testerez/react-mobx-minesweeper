{
  "name": "react-ts",
  "version": "1.0.0",
  "description": "A react starter project using typescript",
  "main": "dist/index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html  --public-url '.'",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "karma start  --single-run --grep",
    "test:w": "karma start --grep",
    "check": "npm run check:ts && npm run lint",
    "check:ts": "tsc --noEmit",
    "lint": "tslint -r node_modules/tslint-microsoft-contrib 'src/**/*.ts' 'src/**/*.tsx'",
    "tsfmt": "find src | grep '\\.tsx?$' -E | xargs tsfmt -r"
  },
  "author": "Tom Esterez",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/enzyme": "^3.1.9",
    "@types/mocha": "^2.2.32",
    "@types/react": "^16.0.40",
    "autoprefixer": "^8.0.0",
    "awesome-typescript-loader": "^1.1.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.3.0",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "install": "^0.8.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^1.7.0",
    "mobx-react-devtools": "^4.2.15",
    "mocha": "^3.1.2",
    "node-sass": "^4.7.2",
    "npm": "^3.10.5",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^4.0.0",
    "tslint": "^3.13.0",
    "tslint-microsoft-contrib": "^2.0.9",
    "typescript": "^2.0.3",
    "typescript-formatter": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-build-notifier": "^0.1.8",
    "webpack-dev-server": "^1.14.1"
